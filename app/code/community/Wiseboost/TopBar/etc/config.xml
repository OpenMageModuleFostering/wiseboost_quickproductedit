<config>
    <modules>
        <Wiseboost_TopBar>
            <version>1.0.0</version>
        </Wiseboost_TopBar>
    </modules>
    <global>
        <blocks>
            <wiseboost_topbar>
                <class>Wiseboost_TopBar_Block</class>
            </wiseboost_topbar>
        </blocks>
		
		<!-- Custom Admin Notifications -->

    </global>
    <frontend>
        <layout>
            <updates>
                <wiseboost_topbar>
                    <file>wiseboost/topbar.xml</file>
                </wiseboost_topbar>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
            <topbar_check>
                <use>admin</use>
                <args>
                    <module>Wiseboost_TopBar</module>
                    <frontName>topbar_check</frontName>
                </args>
            </topbar_check>
        </routers>
    </admin>
	
	
	<!-- Custom Admin Notifications -->

</config>
